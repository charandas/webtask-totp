{
  "name": "webtask-totp",
  "version": "1.0.0",
  "description": "An API to generate TOTP using node and webtask",
  "main": "server.js",
  "semistandard": {
    "ignore": [
      "node_modules/",
      "public/jspm_packages"
    ],
    "globals": [
      "SystemJS"
    ]
  },
  "scripts": {
    "dev": "nodemon",
    "semistandard": "./node_modules/.bin/semistandard",
    "jspm": "./node_modules/.bin/jspm",
    "nodemon": "./node_modules/.bin/nodemon",
    "frontend-local": "jspm bundle app/main.js public/dist/bundle.js -wd",
    "build-prod": "webpack -d",
    "deploy-webtask": "npm run build-prod && wt create build/$(node -e 'console.log(require(\"./package.json\").name)').js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charandas/webtask-totp.git"
  },
  "keywords": [
    "OTP",
    "TOTP",
    "Webtask",
    "QRCode"
  ],
  "author": "Charandas Batra-Daitch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charandas/webtask-totp/issues"
  },
  "homepage": "https://github.com/charandas/webtask-totp#readme",
  "dependencies": {
    "base32.js": "^0.1.0",
    "hi-base32": "^0.2.0"
  },
  "devDependencies": {
    "body-parser": "^1.17.1",
    "cors": "^2.8.1",
    "express": "^4.15.2",
    "jspm": "0.17.0-beta.41",
    "lodash": "^4.17.4",
    "nodemon": "^1.11.0",
    "request-promise": "^4.2.0",
    "semistandard": "^10.0.0",
    "serve-static": "^1.12.1",
    "webpack": "^1.14.0",
    "webtask-tools": "^3.2.0"
  },
  "jspm": {
    "name": "webtask-totp",
    "main": "webtask-totp.js",
    "directories": {
      "baseURL": "public"
    },
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.6.3",
      "angular-animate": "github:angular/bower-angular-animate@^1.6.3",
      "angular-qrcode": "npm:angular-qrcode@^7.2.0",
      "angular-route": "github:angular/bower-angular-route@^1.6.3",
      "axios": "npm:axios@^0.15.3",
      "components/font-awesome": "github:components/font-awesome@^4.7.0",
      "css": "github:systemjs/plugin-css@^0.1.33",
      "lodash": "npm:lodash@^4.17.4",
      "qrcode-generator": "npm:qrcode-generator@^1.1.0",
      "text": "github:systemjs/plugin-text@^0.0.9"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.21"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "buffer": "npm:jspm-nodelibs-buffer@^0.2.0",
      "constants": "npm:jspm-nodelibs-constants@^0.2.0",
      "crypto": "npm:jspm-nodelibs-crypto@^0.2.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "fs": "npm:jspm-nodelibs-fs@^0.2.0",
      "http": "npm:jspm-nodelibs-http@^0.2.0",
      "https": "npm:jspm-nodelibs-https@^0.2.0",
      "os": "npm:jspm-nodelibs-os@^0.2.0",
      "path": "npm:jspm-nodelibs-path@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "stream": "npm:jspm-nodelibs-stream@^0.2.0",
      "string_decoder": "npm:jspm-nodelibs-string_decoder@^0.2.0",
      "url": "npm:jspm-nodelibs-url@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0",
      "vm": "npm:jspm-nodelibs-vm@^0.2.0",
      "zlib": "npm:jspm-nodelibs-zlib@^0.2.0"
    },
    "overrides": {
      "github:angular/bower-angular-animate@1.6.3": {
        "main": "angular-animate",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-animate.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-route@1.6.3": {
        "main": "angular-route",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-route.js": {
            "deps": [
              "angular"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular@1.6.3": {
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global"
          }
        }
      },
      "npm:angular@1.6.3": {
        "format": "global",
        "jspmNodeConversion": false,
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global"
          }
        }
      },
      "npm:browserify-zlib@0.1.4": {
        "dependencies": {
          "readable-stream": "^2.0.2",
          "pako": "~0.2.0"
        },
        "map": {
          "_stream_transform": "readable-stream/transform"
        }
      },
      "npm:debug@2.6.3": {
        "main": "src/browser.js",
        "jspmNodeConversion": false,
        "format": "cjs",
        "map": {
          "./src/browser.js": {
            "node": "./src/node.js"
          },
          "./node.js": {
            "browser": "./src/browser.js"
          },
          "fs": "@node/fs",
          "net": "@node/net",
          "tty": "@node/tty",
          "util": "@node/util"
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:lodash@4.17.4": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:ms@0.7.2": {
        "jspmNodeConversion": false,
        "format": "cjs"
      },
      "npm:qrcode-generator@1.1.0": {
        "meta": {
          "*": {
            "format": "global",
            "exports": "qrcode"
          }
        }
      },
      "npm:angular-qrcode@7.2.0": {
        "meta": {
          "*": {
            "deps": [
              "qrcode-generator"
            ]
          }
        }
      }
    }
  }
}
